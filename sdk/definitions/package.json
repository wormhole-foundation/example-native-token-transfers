{
  "name": "@wormhole-foundation/sdk-definitions-ntt",
  "version": "0.1.0-beta.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/example-native-token-transfers.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/example-native-token-transfers"
  },
  "homepage": "https://github.com/wormhole-foundation/example-native-token-transfers#readme",
  "directories": {
    "test": "__tests__"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "sideEffects": true,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/cjs/index.d.ts",
        "./dist/esm/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json  && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:esm && npm run build:cjs",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -rf ./dist",
    "test": "jest --config ./jest.config.ts"
  },
  "dependencies": {
    "@wormhole-foundation/sdk-base": "^0.8.2",
    "@wormhole-foundation/sdk-definitions": "^0.8.2"
  },
  "peerDependencies": {
    "@wormhole-foundation/sdk-base": "^0.8.2",
    "@wormhole-foundation/sdk-definitions": "^0.8.2"
  },
  "type": "module"
}
